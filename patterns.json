  "__ApiObjectNotFoundOption.doc": {
    "#": "Specifies what the Business API should do when the target record is not found in the database.",
    "throw404": "Responds with a 404 Not Found error. This is the default and most commonly used behavior.",
    "returnNull": "Returns `null` instead of throwing an error, allowing the API logic to continue. Useful for soft reads or optional resources."
  },

