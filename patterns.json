    "__nullables": ["dataObjects"]
  },

  "__AbacPermission.doc": "Configuration for Attribute-Based Access Control (ABAC), allowing fine-grained permission control using attribute-level rules on data objects. ABAC evaluates access by applying custom filter conditions to each object. When a rule matches, the user session may be granted specific permissions or even dynamic roles for that object. These rules can supplement or override standard RBAC or OBAC logic.",
  "AbacPermission": {
    "__attributeBasedPermissionsIsActive.doc": "A boolean value that enables attribute-level access control. When true, data access will be filtered according to ABAC rules defined in `abacDefinitions`.",
    "__abacDefinitions.doc": "An object array to define ABAC (Attribute-Based Access Control) rules. Each rule applies a conditional filter to determine which records a user can access, and grants either dynamic permissions or a role for matching records.",
    "__lines": "\
  a-attributeBasedPermissionsIsActive\
  g-abacDefinitions",
    "attributeBasedPermissionsIsActive": "Boolean",
    "abacDefinitions": ["ABACDefinititon"],
    "__abacDefinitions.single": "abacDefinition",
    "__nullables": ["abacDefinitions"]
  },

  "__ABACDefinititon.doc": "Defines a single ABAC (Attribute-Based Access Control) rule for a given data object. Each rule applies a conditional filter to determine which records a user can access, and grants either dynamic permissions or a role for matching records.",
  "ABACDefinititon": {
    "__dataObject.doc": "The name of the data object to which this ABAC rule applies.",
    "__whereClause.doc": "A JavaScript-style `where` clause defined using MScript syntax. This condition determines which records in the data object will match the rule and grant access.",
    "__name.doc": "The unique identifier for this ABAC rule. It is used in the backend logic, database, and UI for reference.",
    "__permissions.doc": "A list of permission strings that will be granted when the ABAC rule matches a record. These permissions are context-specific and take precedence over less-specific permission types (such as role-based permissions).",
    "name": "String",
    "dataObject": "DataObjectName",
    "whereClause": "MScript",
    "permissions": ["String"],
    "__permissions.single": "permission"
  },
